  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Replay M2",
      "program": "${workspaceFolder}\\bin\\m2-replay",
      "args": [
        "${workspaceFolder}/logs/m2-1572137363514.log"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Start Server",
      "program": "${workspaceFolder}\\bin\\www",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Parse DBC",
      "program": "${workspaceFolder}\\bin\\parse-dbc",
      "args": [".\\dbc\\tesla_model3.dbc"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Monitor Onyx M2",
      "program": "${workspaceFolder}\\bin\\onyx-m2-monitor"
    }

  ]
}